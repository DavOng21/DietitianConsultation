@page "/nutritionlists/view/{id:int}"
@inject HttpClient _client
@inject NavigationManager _navManager

<h3>View</h3>
<FormComponent nutritionlist="nutritionlist" Disabled="true" />
@code {
        [Parameter] public int id { get; set; }
        NutritionList nutritionlist = new NutritionList();

protected async override Task OnParametersSetAsync()
    {
        nutritionlist = await
        _client.GetFromJsonAsync<NutritionList>($"{Endpoints.NutritionListsEndpoint}/{id}");
    }
}
